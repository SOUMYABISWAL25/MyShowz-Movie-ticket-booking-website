{
  "name": "myshowz-movie-ticket-booking-website",
  "version": "1.0.0",
  "scripts": {
    "dev": "npx http-server -p 3000 -o",
    "start": "npx http-server -p 3000",
    "build": "mkdir -p dist && cp -r assets dist/ && cp *.html dist/",
    "amplify:deploy": "npx ampx sandbox",
    "amplify:config": "node update-amplify-config.js",
    "amplify:setup": "npm install && npx ampx sandbox && node update-amplify-config.js"
  },
  "dependencies": {
    "aws-amplify": "^6.15.7",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "aws-cdk-lib": "^2.204.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.11",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
